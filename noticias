      <media:content url="https://imagenes.elpais.com/resizer/v2/PAZ72DYZXZDLDEVPLSPXRNNGBM.jpg?auth=449d6961078d20007fe3e3db5692a1064f71b8751392725f83790b16a26a6a1e" type="image/jpeg" medium="image">
