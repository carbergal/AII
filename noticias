      <media:content url="https://imagenes.elpais.com/resizer/v2/BPZT7AQRCTIJ2IU7ETF5TVPUCE.jpg?auth=c70ab9dea872407763b24f0490be4c0d72c63273c21312d1e372142589cf68d4" type="image/jpeg" medium="image">
