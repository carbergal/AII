      <media:content url="https://imagenes.elpais.com/resizer/v2/3ARKDE2RTFA6XF2HUTCMHLFCMU.jpg?auth=f370039166beab8dbaa68a92569904bf4e939c2f5244305bfe4a852e0418c218" type="image/jpeg" medium="image">
