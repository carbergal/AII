      <media:content url="https://imagenes.elpais.com/resizer/v2/TEOEQJFWBJBXXG24EDTVA4XV6A.jpg?auth=db1135e0f76fd804e3fa9fd5890ae628f3ec23da572b76472fdceb9f46c8d9c1" type="image/jpeg" medium="image">
