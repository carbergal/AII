      <media:content url="https://imagenes.elpais.com/resizer/v2/OAVBKPUJVBEL7ABCY7L6VLU3NM.jpg?auth=6233faaf2263fbb30b27477dffac6575cf7da85858f386234efb358b35f38b0d" type="image/jpeg" medium="image">
