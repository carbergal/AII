      <media:content url="https://imagenes.elpais.com/resizer/v2/GRFVVMKXHJGHZOUKYJJUVXRJVU.jpg?auth=d57022842dbabe19c916902fd9f10797c99371406b57aa98212f57049e0246ee" type="image/jpeg" medium="image">
