      <media:content url="https://imagenes.elpais.com/resizer/v2/7CLJLX3UJRBZNDSZCKQDK4NVKA.jpg?auth=ce1e8c4fb53b2125588d01d840e22b7199d8f5812f9bb7af8e391171a8374179" type="image/jpeg" medium="image">
